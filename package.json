{
  "name": "@tech-j-apprend/type-orm-graphql",
  "version": "1.0.5",
  "description": "",
  "type": "module",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "exports": {
    "require": "./dist/bundle.cjs.js",
    "import": "./dist/bundle.esm.js"
  },
  "scripts": {
    "build:types": "tsc",
    "build": "rollup -c",
    "build:linux": "npm run build:types && mv ./src/templates ./dist/templates",
    "execute": "ts-node -r tsconfig-paths/register ./src/type-orm-graphql.ts",
    "process:graphql": "nodemon",
    "generate": "graphql-codegen",
    "generate:barrels": "barrelsby --delete -d src -e src/cli/.*\\.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "barrelsby": "^2.8.0",
    "dotenv": "^16.1.4",
    "graphql": "^16.6.0",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "nodemon": "^2.0.22",
    "rollup": "^3.23.1",
